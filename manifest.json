{
    "manifest_version":2,
    "name":"auto-refresh",
    "version":"1.0.0",
    "background":{
      "service_worker":"background.js"
    },
    "content_scripts":[
       {
         "matches":["<all_urls>"],
         "js":["script.js"]
       }
    ],
    
    "permissions":["storage","activeTab","contextMenus"],
    "browser_action":{
      "default_popup":"popup.html",
      "default_title":"auto-refresh"
    }
   
}